{
	"_version": "1.58.0",
	"sap.app": {
		"id": "webapp",
		"i18n": "i18n/i18n.properties",
		"type": "application"
		
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "webapp.View.Listagem",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.108.0",
			"libs": {
			  "sap.ui.core": {},
			  "sap.m": {}
			}
		},
		"models": {
			"i18n": {
			  "type": "sap.ui.model.resource.ResourceModel",
			  "settings": {
				"bundleName": "webapp.i18n.i18n",
				"supportedLocales": [""],
            	"fallbackLocale": ""
			  }
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "webapp.View",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"routes": [
				{
					"pattern": "",
					"name": "listagem",
					"target": "listagem"
				},
				{
					"pattern": "detalhes/{detalhesPath}",
					"name": "detalhes",
					"target": "detalhes"
				}
			],
			"targets": {
				"listagem": {
					"id": "listagem",
					"name": "Listagem"
				},
				"detalhes": {
					"id":"detalhes",
					"name": "Detalhes"
				}
			}
		}
	}
}